{
  "rabbit_version": "3.8",
  "users": [
    {
      "name": "admin",
      "password": "123456",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "/",
